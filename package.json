{
  "name": "@laserware/eslint-config",
  "version": "4.0.0-beta.4",
  "description": "ESLint configuration for Laserware.",
  "exports": {
    "./base": "./base.mjs",
    "./svelte": "./svelte.mjs"
  },
  "repository": {
    "url": "git+https://github.com/laserware/eslint-config.git",
    "type": "git"
  },
  "files": [
    "base.mjs",
    "svelte.mjs",
    "rules"
  ],
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "scripts": {
    "version": "git add .",
    "postversion": "git push && git push --tags",
    "prettier": "prettier --write \"./*.mjs\""
  },
  "dependencies": {
    "@eslint/js": "^9.13.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-unicorn": "^56.0.0",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^15.11.0",
    "typescript-eslint": "^8.11.0"
  },
  "devDependencies": {
    "eslint": "^9",
    "eslint-plugin-svelte": "^2.46.0",
    "prettier": "^3.3.3",
    "svelte-eslint-parser": "^0.43.0",
    "typescript": "^5"
  },
  "peerDependencies": {
    "eslint": "^9",
    "typescript": ">=5"
  },
  "optionalDependencies": {
    "eslint-plugin-svelte": "*",
    "svelte-eslint-parser": "*"
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": {
    "trailingComma": "all",
    "printWidth": 100
  }
}
