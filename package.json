{
  "name": "@laserware/eslint-config",
  "version": "3.2.2",
  "description": "ESLint configuration for Laserware.",
  "main": "index.cjs",
  "exports": {
    ".": "./index.cjs",
    "./svelte": "./svelte.cjs"
  },
  "repository": {
    "url": "git+https://github.com/laserware/eslint-config.git",
    "type": "git"
  },
  "files": [
    "index.cjs",
    "svelte.cjs"
  ],
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "scripts": {
    "version": "git add .",
    "postversion": "git push && git push --tags",
    "prettier": "prettier --write \"./*.cjs\""
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-unicorn": "^55.0.0",
    "eslint-plugin-vitest": "^0.4.1",
    "prettier": "^3.3.3",
    "typescript": "^5"
  },
  "devDependencies": {
    "eslint": "^8",
    "typescript": "^5"
  },
  "peerDependencies": {
    "eslint": "^8",
    "typescript": ">=5"
  },
  "optionalDependencies": {
    "eslint-plugin-svelte": "*"
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": {
    "trailingComma": "all"
  }
}
