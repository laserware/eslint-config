{
  "name": "@laserware/eslint-config",
  "version": "5.3.0",
  "description": "ESLint configuration for Laserware.",
  "exports": {
    "./base": "./base.mjs",
    "./svelte": "./svelte.mjs"
  },
  "repository": {
    "url": "git+https://github.com/laserware/eslint-config.git",
    "type": "git"
  },
  "files": [
    "base.mjs",
    "svelte.mjs",
    "rules"
  ],
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "scripts": {
    "version": "git add .",
    "postversion": "git push && git push --tags",
    "prettier": "prettier --write \"./*.mjs\""
  },
  "dependencies": {
    "@eslint/js": "^9.27.0",
    "eslint-import-resolver-typescript": "^4.3.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.1.0",
    "typescript-eslint": "^8.32.1"
  },
  "devDependencies": {
    "eslint": "^9",
    "eslint-plugin-svelte": "^3.9.0",
    "prettier": "^3.5.3",
    "svelte-eslint-parser": "^1.2.0",
    "typescript": "^5"
  },
  "peerDependencies": {
    "eslint": "^9",
    "typescript": ">=5"
  },
  "optionalDependencies": {
    "eslint-plugin-svelte": "*",
    "svelte-eslint-parser": "*"
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": {
    "trailingComma": "all",
    "printWidth": 80
  }
}
